# Add your environment variables here for local development
# Copy this file to .env and fill in your actual values

# Telegram Bot Configuration
TELEGRAM_BOT_TOKEN=your-bot-token-here
TELEGRAM_BOT_USERNAME=your-bot-username-here

# Database Configuration (for production)
DATABASE_URL=jdbc:postgresql://localhost:5432/telegram_bot
DB_USERNAME=postgres
DB_PASSWORD=password

# Server Configuration
PORT=8080
SPRING_PROFILES_ACTIVE=dev

# Optional: Webhook configuration for production
WEBHOOK_SECRET_TOKEN=your-webhook-secret-token
